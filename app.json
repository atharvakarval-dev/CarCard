{
  "expo": {
    "name": "CarCard",
    "slug": "car-card",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "carcard",
    "userInterfaceStyle": "automatic",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow CarCard to access your camera for scanning tags."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}